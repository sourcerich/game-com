{
  "env": {
      "node": true,
      "es2022": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "eslint-plugin-etc"
  ],
  "rules": {
      // eslint
      "dot-notation": "error",
      "etc/no-commented-out-code": "error",
      "@typescript-eslint/no-explicit-any": "off"
  }
}